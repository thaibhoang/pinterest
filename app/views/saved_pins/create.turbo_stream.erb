<%= turbo_stream.replace "box_#{dom_id(@pin)}" do %>
  <%= render partial: "shared/pin", locals: {pin: @pin, saved_pin: @saved_pin} %>
<% end %>

<%= turbo_stream.replace "original_box_#{dom_id(@pin)}" do %>
  <%= render partial: "shared/pin", locals: {pin: @pin} %>
<% end %>

<%= turbo_stream.update "save_unsaved#{dom_id(@pin)}" do %>
  <%= render partial: "shared/saved_overlay", locals: { pin: @pin } %>
<% end %>

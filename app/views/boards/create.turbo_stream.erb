<%= turbo_stream.replace "board_id" do %>
  <%= render partial: "pins/select_tag", locals: { options: @options, new_board_id: @new_board_id, new_board_name: @new_board_name } %>
<% end %>

<%= turbo_stream.replace "new_board" do %>
  <%= turbo_frame_tag "new_board" %>
<% end %>

<%= turbo_stream.replace "all-boards" do %>
  <%= render partial: "shared/all_boards", locals: {boards: @boards, user: @user} %>
<% end %>

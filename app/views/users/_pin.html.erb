<div class="container">
  <% cache pins do %>
    <% pins.each do |pin| %>
      <% cache pin do %>
        <% if pin.image.attached? %>
          <div class="box"><%= link_to image_tag(pin.resize_img(pin.image, 236, 760)), pin , data: { turbo_frame: "_top" } %></div>
        <% end %>
      <% end %>
    <% end %>
  <% end %>
</div>



  